<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="footer">
            <block class="Magento\Backend\Block\Page\Footer" name="ityetti.profiler" after="legal"
                   template="ITYetti_DatabaseProfiler::html/profiler.phtml">
                <arguments>
                    <argument name="ityetti_profiler" xsi:type="object">
                        ITYetti\DatabaseProfiler\ViewModel\Profiler
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
